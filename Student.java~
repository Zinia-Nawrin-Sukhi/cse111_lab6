class Student{
    public String name="Ei name e kono Student nai";
    public String address="Student e nai abr id:p";
    public String id="Naam nai .. thikana ashbe koi theke?";
    public double cgpa=-4.0;
    public  Student(){
    }
    public  Student(String name,String id,String address,double cgpa){
         this.name=name;
         this.address=address;
         this.id=id;
         this.cgpa=cgpa;
    }
    public String nameDao(){
        return name;
    }
    public String addressDao(){
        return address;
    }
    public String boloToAmiKe(){
        return id;
    }
    public double cgpaDao(){
        return cgpa;
    }
     public String nameDao(String name){
        this.name=name;
        return name;
    }
    public String addressDao(String address){
        this.address=address;
        return address;
    }
    public String boloToAmiKe(String id){
        this.id=id;
        return id;
    }
    public double cgpaDao(double cgpa){
        this.cgpa=cgpa;
        return cgpa;
    }
    public void standUp(){
        System.out.println(name+"  is now standing up");
    }
    public String tellMeYourName(){
        return "sir hi i am "+name;
    }
    public void call(String nam){
        System.out.println(name+" hey "+nam+" sir is calling you");
    }
    public int add2Numbers(int num1,num2){
        int num3=num1+num2;
        return num3;
    }
}
    
    